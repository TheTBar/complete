<ul id="product-thumbnails" class="thumbnails list-inline" data-hook>
  <% @products.each do |p| %>
    <% p.images.each do |i| %>
      <li class='tmb-all tmb-<%= i.viewable.id %>'>
        <%= link_to(image_tag(i.attachment.url(:mini), class: "thumbnail"), i.attachment.url(:product)) %>
      </li>
    <% end %>
  <% end %>
</ul>

